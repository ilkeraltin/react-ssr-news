{
  "name": "react-news-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "engines": {
    "node": "10.14.2"
  },
  "main": "index.js",
  "scripts": {
    "start": "node build/bundle.js",
    "build:prod": "rm -rf build && rm -rf public && webpack  --config webpack.server.js && webpack -p --config webpack.client.prod.js --mode production",
    "heroku-postbuild": "npm install && npm run build:prod",
    "webpack-prod": "webpack -p --progress --config webpack.client.prod.js --mode production",
    "dev": "npm run build-server && npm-run-all --parallel dev:*",
    "build-server": "webpack --config webpack.server.js",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "precommit": "pretty-quick --staged"
  },
  "author": "ilker ALTIN",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "axios": "0.19.0",
    "compression": "^1.7.3",
    "compression-webpack-plugin": "^2.0.0",
    "concurrently": "4.1.0",
    "express": "4.17.1",
    "lodash": "4.17.13",
    "nodemon": "1.18.10",
    "npm-run-all": "4.1.5",
    "prop-types": "^15.7.2",
    "react-dom": "16.9.0",
    "react": "16.9.0",
    "react-helmet": "5.2.0",
    "react-lazy-load-image-component": "^1.3.2",
    "react-redux": "6.0.0",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.3.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "serialize-javascript": "1.6.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "4.39.2",
    "webpack-dev-server": "3.8.0",
    "webpack-merge": "4.2.1",
    "webpack-node-externals": "1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.7",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^2.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.2.3"
  }
}
